{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "name",
    "job",
    "id",
    "createdAt"
  ],
  "properties": {
    "name": {
      "type": "string",
      "default": "",
      "title": "The name Schema",
      "examples": [
        "morpheus"
      ]
    },
    "job": {
      "type": "string",
      "default": "",
      "title": "The job Schema",
      "examples": [
        "leader"
      ]
    },
    "id": {
      "type": "string",
      "default": "",
      "title": "The id Schema",
      "examples": [
        "303"
      ]
    },
    "createdAt": {
      "type": "string",
      "default": "",
      "title": "The createdAt Schema",
      "examples": [
        "2023-12-29T00:38:35.220Z"
      ]

  },
  "examples": [{
    "name": "morpheus",
    "job": "leader",
    "id": "303",
    "createdAt": "2023-12-29T00:38:35.220Z"
  }]
}